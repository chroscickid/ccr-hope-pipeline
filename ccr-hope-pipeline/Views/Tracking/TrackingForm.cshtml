@model HopePipeline.Models.DbEntities.Tracking.TrackingForm;
@{
    ViewData["Title"] = "StartIntake";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";

}

<!DOCTYPE html>
<html>
<head>
    <style>
        table {
            width: 100%;
            border: 3px solid #003A62;
            border-collapse: collapse;
        }

        th, td {
            border: 1px solid black;
            border-collapse: collapse;
        }

        th, td {
            padding: 15px;
            text-align: left;
        }

        table#t01 tr:nth-child(even) {
            background-color: #e6f3ff;
        }

        table#t01 tr:nth-child(odd) {
            background-color: #fff;
        }

        table#t01 th {
            background-color: #005199;
            color: white;
        }

        #ifYes, #ifNo {
            display: none;
        }

        #yesCheck:checked ~ #ifYes {
            display: block;
        }

        #noCheck:checked ~ #ifNo {
            display: block;
        }

        #ifYe, #ifN {
            display: none;
        }

        #yesGrade:checked ~ #ifYe {
            display: block;
        }

        #noGrade:checked ~ #ifN {
            display: block;
        }

        #ifYesIEP, #ifNoIEP {
            display: none;
        }

        #yesIEP:checked ~ #ifYesIEP {
            display: block;
        }

        #noIEP:checked ~ #ifNoIEP {
            display: block;
        }

        #ifYesComp, #ifNoComp {
            display: none;
        }

        #yesComp:checked ~ #ifYesComp {
            display: block;
        }

        #noComp:checked ~ #ifNoComp {
            display: block;
        }

        #ifYesBullied, #ifNoBullied {
            display: none;
        }

        #yesBullied:checked ~ #ifYesBullied {
            display: block;
        }

        #noBullied:checked ~ #ifNoBullied {
            display: block;
        }

        #ifYesReport, #ifNoReport {
            display: none;
        }

        #yesRep:checked ~ #ifYesReport {
            display: block;
        }

        #noRep:checked ~ #ifNoReport {
            display: block;
        }

        #ifYesAlt, #ifNoAlt {
            display: none;
        }

        #yesAlt:checked ~ #ifYesAlt {
            display: block;
        }

        #noAlt:checked ~ #ifNoAlt {
            display: block;
        }

        #ifYesRef, #ifNoRef {
            display: none;
        }

        #yesRef:checked ~ #ifYesRef {
            display: block;
        }

        #noRef:checked ~ #ifNoRef {
            display: block;
        }

        #ifYesSus, #ifNoSus {
            display: none;
        }

        #yesSus:checked ~ #ifYesSus {
            display: block;
        }

        #noSus:checked ~ #ifNoSus {
            display: block;
        }
        .tab {
            overflow: hidden;
            border: 1px solid #ccc;
            background-color: #f1f1f1;
            position: fixed;
            width: 100%;
            top: 55px;
        }

            /* Style the buttons inside the tab */
            .tab button {
                background-color: inherit;
                float: left;
                border: none;
                outline: none;
                cursor: pointer;
                padding: 14px 16px;
                transition: 0.3s;
                font-size: 17px;
                display: block;
            }

        

        /* Change background color of buttons on hover */
        .tab button:hover {
            background-color: #ddd;
        }

        /* Create an active/current tablink class */
        .tab button.active {
            background-color: #ccc;
        }

        /* Style the tab content */
        .tabcontent {
            display: none;
            padding: 6px 12px;
            border: 1px solid #ccc;
            border-top: none;
        }
    </style>

    <script>
        function pagination(id) {
            var page1 = document.getElementById("page1");
            var page2 = document.getElementById("page2");
            var page3 = document.getElementById("page3");
            var page4 = document.getElementById("page4");
            var tab1 = document.getElementById("tab1");
            var tab2 = document.getElementById("tab2");
            var tab3 = document.getElementById("tab3");
            var tab4 = document.getElementById("tab4");

            switch (id) {
                case "tab1":
                    page1.style.display = "block";
                    page2.style.display = "none";
                    page3.style.display = "none";
                    page4.style.display = "none";
                    tab1.className = "tablinks active";
                    tab2.className = "tablinks";
                    tab3.className = "tablinks";
                    tab4.className = "tablinks";
                    break;
                case "tab2":
                    page1.style.display = "none";
                    page2.style.display = "block";
                    page3.style.display = "none";
                    page4.style.display = "none";
                     tab1.className = "tablinks";
                    tab2.className = "tablinks active";
                    tab3.className = "tablinks";
                    tab4.className = "tablinks";
                    break;
                case "tab3":
                    page1.style.display = "none";
                    page2.style.display = "none";
                    page3.style.display = "block";
                    page4.style.display = "none";
                     tab1.className = "tablinks";
                    tab2.className = "tablinks";
                    tab3.className = "tablinks active";
                    tab4.className = "tablinks";
                    break;
                case "tab4":
                    page1.style.display = "none";
                    page2.style.display = "none";
                    page3.style.display = "none";
                    page4.style.display = "block";
                    tab1.className = "tablinks";
                    tab2.className = "tablinks";
                    tab3.className = "tablinks";
                    tab4.className = "tablinks active";
                    break;
            }
        }
    </script>
</head>
<body>

    <h1> Center for Children's Rights Tracking Form </h1>
    <div class="tab">
        <nav aria-label="Page navigation example">
            
                <button class="tablinks active" onclick="pagination(this.id)" id="tab1">Demographic Information</button>
                <button class="tablinks" onclick="pagination(this.id)" id="tab2">School Information</button>
                <button class="tablinks" onclick="pagination(this.id)" id="tab3">Discipline Information</button>
                <button class="tablinks" onclick="pagination(this.id)" id="tab4">Center for Children's Rights Information</button>
            
        </nav>
    </div>
    <form asp-controller="Tracking" asp-action="SubmitTracking" method="post">
        <div asp-validation-summary="ModelOnly"></div>
        <input asp-for="ClientID" type="hidden" class="field-style field-full align-none" placeholder="Client ID" value="@Model.ClientID" />
        @{
            var refMove = Model.referralBrandi;
        }
        <div id="page1">
            <table id="t01">
                <tr>
                    <th colspan="3">Demographic Information</th>
                </tr>
                <tr>
                    <td>
                        <label asp-for="reffname">Referral Source's First Name: </label><br />
                        <input asp-for="reffname" type="text" name="referral" placeholder="Referral Source First Name" value="@refMove.referralfname" maxlength="20" /></br>
                        <label asp-for="reflname">Referral Source's Last Name:</label>
                        <input asp-for="reflname" type="text" name="referral" placeholder="Referral Source Last Name" value="@refMove.referrallname" maxlength="20" />
                    </td>

                    <td>
                        <label asp-for="clientFirstName">Youth First Name:</label><br />
                        <input asp-for="clientFirstName" type="text" value="@refMove.fName" required><br><br />
                        <label asp-for="clientLastName">Youth Last Name:</label><br />
                        <input asp-for="clientLastName" type="text" placeholder="Last Name" value="@refMove.lName" required><br><br />
                    </td>

                    <td>
                        <label asp-for="clientDOB"> Date of Birth: </label><br />
                        <input asp-for="clientDOB" type="date" min="2001-01-01" max="2050-12-31" class="field-style field-full align-none" placeholder="Date of Birth" value="@refMove.dOB" required> <br />
                    </td>

                </tr>
            </table>
            <table id="t01" th colspan="4">
                <tr>
                    <td>
                        <label asp-for="clientEthnicity">Ethnicity:</label> <br>
                        <select asp-for="clientEthnicity" class="field-style field-full align-none" required>
                            <option value="nhWhite">Non Hispanic White</option>
                            <option value="hispanic">Hispanic</option>
                            <option value="natAm">Native American</option>
                            <option value="jewish">Jewish</option>
                            <option value="black">Black</option>
                            <option value="multi">Multiple Ethnicities</option>
                            <option value="notListed">Ethnicity Not Listed</option>
                        </select>
                    </td>

                    <td colspan="3">

                        <label asp-for="clientGender">Gender Identity: </label><br />

                        @{ if (refMove.gender == "Male")
                            { <input asp-for="clientGender" type="radio" value="male" checked>}
                            else
                            { <input asp-for="clientGender" type="radio" value="male">} } He / Him / His &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;

                        @{ if (refMove.gender == "female")
                            { <input asp-for="clientGender" type="radio" value="female" checked>}
                            else
                            { <input asp-for="clientGender" type="radio" value="female">} } She/Her/Hers &nbsp;&nbsp;&nbsp;
                        @{ if (refMove.gender == "trans*")
                            { <input asp-for="clientGender" type="radio" value="trans" checked>}
                            else
                            { <input asp-for="clientGender" type="radio" value="trans">} } They/Them/Theirs <br>
                        @{ if (refMove.gender == "nonbinaryF")
                            { <input asp-for="clientGender" type="radio" value="nonbinaryF" checked>}
                            else
                            { <input asp-for="clientGender" type="radio" value="nonbinaryF">} } She/They &nbsp; &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                        @{ if (refMove.gender == "nonbinaryM")
                            { <input asp-for="clientGender" type="radio" value="nonbinaryM" checked>}
                            else
                            { <input asp-for="clientGender" type="radio" value="nonbinaryM">} } He/They &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                        @{ if (refMove.gender == "neutral")
                            { <input asp-for="clientGender" type="radio" value="neutral" checked>}
                            else
                            { <input asp-for="clientGender" type="radio" value="neutral">} } Zie/Zir/Zirs <br />
                    </td>

                    <td>
                       <label asp-for="school">School:</label> <br>
                        <input asp-for="school" type="text" class="field-style field-full align-none" name="school" value="@refMove.school" required />
                    </td>
                </tr>

                <tr>
                    <td colspan="3">
                        <label asp-for="currentGrade">Grade Level:</label>
                        <br />
                        <select asp-for="currentGrade" class="field-style field-full align-none" placeholder="Grade Level" required>
                            <option value="0" label="K">Kindergarten</option>
                            <option value="1" label="1">1st Grade</option>
                            <option value="2" label="2">2nd Grade</option>
                            <option value="3" label="3">3rd Grade</option>
                            <option value="4" label="4">4th Grade</option>
                            <option value="5" label="5">5th Grade</option>
                            <option value="6" label="6">6th Grade</option>
                            <option value="7" label="7">7th Grade</option>
                            <option value="8" label="8">8th Grade</option>
                            <option value="9" label="9">9th Grade</option>
                            <option value="10" label="10">10th Grade</option>
                            <option value="11" label="11">11th Grade</option>
                            <option value="12" label="12">12th Grade</option>
                        </select>
                    </td>

                    <td colspan="2">
                        <label asp-for="failedGrade">Failed a Grade:</label><br />
                        <input asp-for="failedGrade" type="radio" id="yesGrade" value="1" /> Yes
                        <br />
                        <input asp-for="failedGrade" type="radio" id="noGrade" value="0" checked /> No
                        <br />
                        <br>
                        <br>

                        <div id="ifYe">
                           <label asp-for="whichGradeFailed">If yes, which grade?</label><br />
                            <select asp-for="whichGradeFailed" id="mathLevel" class="field-style field-full align-none">
                                <option value="0" label="0">Kindergarten</option>
                                <option value="1" label="1">1st Grade</option>
                                <option value="2" label="2">2nd Grade</option>
                                <option value="3" label="3">3rd Grade</option>
                                <option value="4" label="4">4th Grade</option>
                                <option value="5" label="5">5th Grade</option>
                                <option value="6" label="6">6th Grade</option>
                                <option value="7" label="7">7th Grade</option>
                                <option value="8" label="8">8th Grade</option>
                                <option value="9" label="9">9th Grade</option>
                                <option value="10" label="10">10th Grade</option>
                                <option value="11" label="11">11th Grade</option>
                                <option value="12" label="12">12th Grade</option>
                            </select>
                            <br />
                            &nbsp;

                            <br />
                            <label asp-for="failCount">How many times?</label><br />
                            <input asp-for="failCount" type="number" />
                            <br />
                            <br />
                        </div>

                        <div id="ifN">

                        </div>
                    </td>
                </tr>

            </table>

            <table id="t01" th colspan="3">
                <tr>
                    <td>
                        <label asp-for="careFirstName">Primary Caretaker's First Name:</label><br />
                        <input asp-for="careFirstName" class="field-style field-full align-none" placeholder="Caregiver First Name" value="@refMove.guardianName" required> <br /><br />
                        <label asp-for="careLastName">Primary Caretaker's Last Name:</label><br />
                        <input asp-for="careLastName" asptype="text" name="careFirstName" class="field-style field-full align-none" placeholder="Caregiver Last Name" value="@refMove.guardianName" required>
                    </td>

                    <td>
                      <label asp-for="careGender">  What is the primary caretaker's relationship to the youth? </label><br />
                        @{ if (refMove.guardianRelationship == "Mother")
                            {
                                <input asp-for="careGender" type="radio" value="Mother" checked>
                            }
                            else
                            {
                                <input asp-for="careGender" type="radio" value="Mother">
                            } } Mother
                        <br />
                        @{ if (refMove.guardianRelationship == "Father")
                            {
                                <input asp-for="careGender" type="radio" value="Father" checked>
                            }
                            else
                            {
                                <input asp-for="careGender" type="radio" value="Father">
                            } } Father
                        <br />

                        <input asp-for="careGender" type="radio" value="mGrandma"> Grandmother
                        <br>
                        <input asp-for="careGender" type="radio" value="mGrandpa"> Grandfather
                        <br>
                        <input asp-for="careGender" type="radio" value="mGrandpa"> Other
                        <br>
                        <br>


                    </td>

                    <td>
                        <label asp-for="careEthnicity">Caretaker Ethnicity:</label> <br />
                        <select asp-for="careEthnicity" class="field-style field-full align-none" required>
                            <option value="nhWhite">Non Hispanic White</option>
                            <option value="hispanic">Hispanic</option>
                            <option value="natAm">Native American</option>
                            <option value="jewish">Jewish</option>
                            <option value="black">Black</option>
                            <option value="multi">Multiple Ethnicities</option>
                            <option value="notListed">Ethnicity Not Listed</option>
                        </select>
                        <br>
                        <div class="form-group">
                        <label class="question" asp-for="carePhone">Caretaker Phone Number:</label> <br />
                        <input type="tel" asp-for="carePhone" pattern="[0-9]{3}-[0-9]{2}-[0-9]{3}" class="field-style field-full align-none" required />
                            </br>
                        <small>Formatted: 555-555-5555</small>
                            </div>
                    </td>
                </tr>

                <tr>
                    <td>
                       <label asp-for="baker">Baker Acted?</label><br />
                        <input asp-for="baker" type="radio" value="1"> Yes
                        <br />
                        <input asp-for="baker" type="radio" value="0" checked /> No
                    </td>

                    <td>
                        <label asp-for="marchman">Marchman Acted?</label><br />
                        <input asp-for="marchman" type="radio" value="1"> Yes
                        <br />
                        <input asp-for="marchman" type="radio" value="0" checked /> No
                    </td>

                    <td>
                       <label asp-for="femHouse">Female-led household?</label><br />
                        <input asp-for="femHouse" type="radio" value="1"> Yes
                        <br />
                        <input asp-for="femHouse" type="radio" value="0" checked /> No
                    </td>
                </tr>


                <tr>
                    <td>
                        <label asp-for="domVio">Domestic Violence?</label><br />
                        <input asp-for="domVio" type="radio" value="1"> Yes
                        <br />
                        <input asp-for="domVio" type="radio" value="0" checked /> No
                    </td>

                    <td>
                       <label asp-for="adopted"> Adopted?</label><br />
                        <input asp-for="adopted" type="radio" value="1"> Yes
                        <br />
                        <input asp-for="adopted" type="radio" value="0" checked /> No
                    </td>

                    <td>
                        <label asp-for="evicted">Housing instablity?</label><br />
                        <input asp-for="evicted" type="radio" value="1"> Yes
                        <br />
                        <input asp-for="evicted" type="radio" value="0" checked /> No
                    </td>
                </tr>

                <tr>
                    <td>
                        <label asp-for="incarParent">Incarcarated Parent?</label><br />
                        <input asp-for="incarParent" type="radio" value="1"> Yes
                        <br />
                        <input asp-for="incarParent" type="radio" value="0" checked /> No
                    </td>

                    <td>
                        <label asp-for="publicAssistance">Receiving public assistance?</label><br />
                        <input asp-for="publicAssistance" type="radio" value="1"> Yes
                        <br />
                        <input asp-for="publicAssistance" type="radio" value="0" checked /> No
                    </td>

                    <td>
                       <label asp-for="asthma">Asthma</label><br />
                        <input asp-for="asthma" type="radio" value="1"> Yes
                        <br />
                        <input asp-for="asthma" type="radio" value="0" checked /> No
                    </td>
                </tr>
            </table>

        </div>




        <div id="page2" hidden>
            <table id="t01">

                <tr>
                    <th colspan="3">School Information </th>
                </tr>

                <tr>
                    <td>
                        <label asp-for="schoolRef">School at time of Referral</label>
                        <br />
                        <input asp-for="schoolRef" type="text" class="field-style field-full align-none" value="@refMove.school" required />
                        <br />
                    </td>


                    <td colspan="2">
                        <label asp-for="readingLevel">Current Reading Level</label>
                        <br />
                        <select asp-for="readingLevel" id="readingLevel" class="field-style field-full align-none" placeholder="Reading Level" required>
                            <option value="13" label="Other"></option>
                            <option value="0" label="K"></option>
                            <option value="1" label="1"></option>
                            <option value="2" label="2"></option>
                            <option value="3" label="3"></option>
                            <option value="4" label="4"></option>
                            <option value="5" label="5"></option>
                            <option value="6" label="6"></option>
                            <option value="7" label="7"></option>
                            <option value="8" label="8"></option>
                            <option value="9" label="9"></option>
                            <option value="10" label="10"></option>
                            <option value="11" label="11"></option>
                            <option value="12" label="12"></option>
                        </select>
                        <br />
                        <br />

                       <label asp-for="mathLevel">Current Math Level</label>
                        <br />
                        <select asp-for="mathLevel" id="mathLevel" class="field-style field-full align-none" required>
                            <option value="13" label="Other"></option>
                            <option value="K" label="K"></option>
                            <option value="1" label="1"></option>
                            <option value="2" label="2"></option>
                            <option value="3" label="3"></option>
                            <option value="4" label="4"></option>
                            <option value="5" label="5"></option>
                            <option value="6" label="6"></option>
                            <option value="7" label="7"></option>
                            <option value="8" label="8"></option>
                            <option value="9" label="9"></option>
                            <option value="10" label="10"></option>
                            <option value="11" label="11"></option>
                            <option value="12" label="12"></option>
                        </select>
                        <br />
                        <br />
                    </td>
                </tr>

                <tr>
                    <td>
                       <label asp-for="iep">Does the youth have an IEP/504 Plan?</label><br />
                        <input asp-for="iep" type="radio" id="yesIEP" value="1"> Yes <br />
                        <input asp-for="iep" type="radio" id="noIEP" value="0" required /> No<br />

                        <div id="ifYesIEP">
                            <label asp-for="iepplan1">Primary IEP/504 Plan</label><br />
                            <input asp-for="iepplan1" type="radio" id="emo" value="emo"> Emotional/Behavioral Disability<br />
                            <input asp-for="iepplan1" type="radio" id="gifted" value="gifted" /> Gifted<br />
                            <input asp-for="iepplan1" type="radio" id="intellectual" value="intell"> Intellectual Disability<br />
                            <input asp-for="iepplan1" type="radio" id="language" value="lang" /> Language Impairment<br />
                            <input asp-for="iepplan1" type="radio" id="otherHealth" value="otherHealth" /> Other Health Impairment<br />
                            <input asp-for="iepplan1" type="radio" id="specificLearning" value="specificLearning"> Specific Learning Disability<br />
                            <input asp-for="iepplan1" type="radio" id="speech" value="speech" /> Speech Impairment<br />
                            <input asp-for="iepplan1" type="radio" id="other" value="other"> Other <br />
                            <br />
