                    <td>
                        <label asp-for="pregnantparenting">Is the student pregnant/parenting?</label>
                        <input asp-for="pregnantparenting" type="radio" value="1"/>Yes <br/>
                        <input asp-for="pregnantparenting" type="radio" value="0" />No
